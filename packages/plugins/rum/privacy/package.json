{
    "name": "@dd/rum-privacy-plugin",
    "packageManager": "yarn@4.0.2",
    "license": "MIT",
    "private": true,
    "author": "Datadog",
    "description": "Allowlist masking for Datadog RUM",
    "homepage": "https://github.com/DataDog/build-plugins/tree/main/packages/plugins/rum/privacy#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/DataDog/build-plugins",
        "directory": "packages/plugins/rum/privacy"
    },
    "exports": {
        ".": "./src/index.ts",
        "./*": "./src/*.ts"
    },
    "toBuild": {
        "privacy-helpers": {
            "format": [
                "cjs",
                "esm"
            ],
            "entry": "./src/built/privacy-helpers/index.ts"
        }
    },
    "scripts": {
        "typecheck": "tsc --noEmit",
        "build": "tsc"
    },
    "dependencies": {
        "@datadog/js-instrumentation-wasm": "^0.9.4",
        "@dd/core": "workspace:*",
        "@rollup/pluginutils": "5.1.4"
    },
    "devDependencies": {
        "typescript": "5.4.3"
    }
}
