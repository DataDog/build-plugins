{
    "name": "@dd/rum-privacy-plugin",
    "packageManager": "yarn@4.0.2",
    "license": "MIT",
    "private": true,
    "author": "Datadog",
    "description": "Allowlist masking for Datadog RUM",
    "homepage": "https://github.com/DataDog/build-plugins/tree/main/packages/plugins/rum-privacy#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/DataDog/build-plugins",
        "directory": "packages/plugins/rum-privacy"
    },
    "exports": {
        ".": "./src/index.ts",
        "./*": "./src/*.ts"
    },
    "scripts": {
        "typecheck": "tsc --noEmit",
        "build": "tsc && yarn build:helpers && yarn move:helpers",
        "clean": "rm -rf ./src/generated/*",
        "build:helpers": "tsc src/privacy-helpers.ts --target es2020 --lib esnext,DOM --module esnext --moduleResolution node --resolveJsonModule --strict --strictNullChecks --esModuleInterop --outDir src/generated",
        "move:helpers": "mv src/generated/privacy-helpers.js src/generated/privacy-helpers.js-txt"
    },
    "dependencies": {
        "@datadog/js-instrumentation-wasm": "0.9.2",
        "@dd/core": "workspace:*",
        "@rollup/pluginutils": "5.1.4"
    },
    "devDependencies": {
        "typescript": "5.4.3"
    }
}
