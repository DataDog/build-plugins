        "@dd/internal-injection-plugin": "workspace:*",
        "@dd/internal-true-end-plugin": "workspace:*",
        "@dd/metrics-plugin": "workspace:*",
        "@dd/output-plugin": "workspace:*",
        "@dd/rum-plugin": "workspace:*",
        "chalk": "2.3.1",
        "unplugin": "2.3.5"
